<div class="form-group row">
    <label class="col-form-label col-sm-2" for="cusId">Customer ID</label>
    <div class="col-sm-8">
      <input autofocus class="form-control" required name="cusId" value="<%= model.cusId %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="cusFname">Customer First Name</label>
    <div class="col-sm-8">
      <input class="form-control" required name="cusFname" value="<%= model.cusFname %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="cusLname">Customer Last Name</label>
    <div class="col-sm-8">
      <input class="form-control" required name="cusLname" value="<%= model.cusLname %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="cusState">Customer State</label>
    <div class="col-sm-8">
      <input class="form-control" name="cusState" value="<%= model.cusState %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="cusSalesYTD">Customer Sales YTD</label>
    <div class="col-sm-8">
      <input class="form-control" name="cusSalesYTD" value="<%= model.cusSalesYTD %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="cusSalesPrev">Customer Sales Previous</label>
    <div class="col-sm-8">
      <input class="form-control" name="cusSalesPrev" value="<%= model.cusSalesPrev %>" />
    </div>
  </div>